<UserControl x:Class="HappyHour.View.BrowserHeaderGoogleEarth"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HappyHour.View"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="500">
    <DockPanel>
        <Button DockPanel.Dock="Right" Content="Open NMEA" Margin="2,0,0,0"
                Command="{Binding CmdOpenNmea}"/>
        <TextBox KeyUp="TextBox_KeyEnterUpdate" Text="{Binding Address}"/>
    </DockPanel>
</UserControl>
